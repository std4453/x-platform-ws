{
    "extends": "airbnb-base",
    "rules": {
        "indent": ["error", 4],
        "linebreak-style": ["error", "windows"],
        "function-paren-newline": "off",
        "object-curly-newline": "off",
        "no-underscore-dangle": "off"
    },
    "env": {
        "node": true,
        "browser": true
    }
}
